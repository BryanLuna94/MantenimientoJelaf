<div class="table-responsive">
    <table class="table color-table table-striped inverse-table font-12">
        <thead>
            <tr>
                <th align="center">CODIGO</th>
                <th align="center">ARTICULO</th>
                <th align="center">CANTIDAD</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <tr class="header" v-for="itemRepuesto in list.Repuestos">
                <td><a href="javascript:void(0)" @@click="selectRepuesto(itemRepuesto)">{{ itemRepuesto.CodigoOriginal }}</a></td>
                <td>{{ itemRepuesto.Descripcion }}</td>
                <td>{{ itemRepuesto.Cantidad }}</td>
                <td width="10%">
                    <a href="#" class="btn btn-danger btn-sm" v-on:click.prevent="EliminarRepuesto(itemRepuesto)">Eliminar</a>
                </td>
            </tr>
        </tbody>
    </table>
</div>